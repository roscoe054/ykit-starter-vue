{
    "presets": [
        ["env", {
            "modules": "commonjs",
            "targets": {
                "browsers": [
                    "> 1%",
                    "last 3 versions",
                    "ios 8",
                    "android 4.2",
                    "ie 9"
                ]
            },
            "useBuiltIns": "usage"
        }]
    ],
    "plugins": [
        "transform-decorators-legacy",
        "transform-class-properties",
        "transform-object-rest-spread",
        "transform-object-assign"
    ]
}
